<div class="site-wrapper not-landing">
  <div class="site-wrapper-inner">
    <div class="cover-container">
      <div class="masthead clearfix">
        <div class="inner">
          <h3 class="masthead-brand">
            Mule
          </h3>
        </div>
      </div>

    </div>
  </div>
</div>
<div class="row footer" style="margin: 30px 0">
    <h1 class="footer">Would you like us to pack for you?</h1>
</div>

<div class="row highlights residence-type">
  <div class="col-md-4 col-md-offset-2">
      <a href="/addresses" class="hoverable service-type-next">
          <%= image_tag "self_service_icon_aqua_240.png", class: "img-responsive" %>
              <h1>No</h1>
              </a>

  </div>
  <div class="col-md-4">
      <a href="/addresses" class="hoverable service-type-next">
          <%= image_tag "full_service_icon_aqua_240.png", class: "img-responsive" %>
              <h1>Yes</h1>
              <p>(This will cost extra)</p>
              </a>

              </div>
</div>
<div class="foot-space" style="padding: 20px; height: 70px">
    <a href="https://mixpanel.com/f/partner" class="pull-right">
        <img src="//cdn.mxpnl.com/site_media/images/partner/badge_blue.png" alt="Mobile Analytics" />
    </a>
</div>

<script>
    mixpanel.track('three: pageview (service)', {
        'page name' : document.title,
        'url' : window.location.pathname
    });

    mixpanel.track_links(".service-type-next", "three: next click", {
        "referrer": document.referrer
    });
</script>
